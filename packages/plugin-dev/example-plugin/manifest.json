{
  "name": "Example Plugin",
  "id": "example-plugin",
  "manifestVersion": 1,
  "version": "1.0.0",
  "minSupVersion": "13.0.0",
  "description": "An example plugin demonstrating TypeScript development workflow",
  "author": "Your Name",
  "hooks": ["taskComplete", "taskUpdate"],
  "permissions": [
    "getTasks",
