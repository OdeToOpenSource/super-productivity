{
  "name": "example-super-productivity-plugin",
  "version": "1.0.0",
  "description": "Example plugin for Super Productivity with TypeScript support",
  "main": "dist/plugin.js",
  "scripts": {
    "dev": "webpack --watch --mode development",
    "build": "webpack --mode production",
    "clean": "rm -rf dist/*",
    "package": "node scripts/package.js",
    "test": "jest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "install-local": "npm run build && cp dist/* ../../../src/assets/example-plugin/",
    "release": "npm run clean && npm run build && npm run package"
  },
  "keywords": [
    "super-productivity",
    "plugin",
    "productivity",
    "task-management"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@super-productivity/plugin-api": "file:../../plugin-api",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "archiver": "^6.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.0.0",
    "typescript": "^5.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
